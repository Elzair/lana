{
  "bitbucket": {
    "/repo/path/": {
        "branch": {
            "dir": "/local/path"
          , "test": true
        }
      , "other_branch": {
            "dir": "/other/branch/dir"
          , "test": false
          , "auth": {
                "method": "ssh"
              , "public_key": "/tmp/.ssh/id_rsa.pub"
              , "private_key": "/tmp/.ssh/id_rsa"
            }
        }
    }
  }
}